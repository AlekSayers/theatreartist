.ruzzle-cell-draggable,
.ruzzle-cell-puttable {
  display: inline;
  position: relative; }

.ruzzle-cell-points-sd,
.ruzzle-cell-points-dd {
  position: absolute;
  top: -38px;
  display: inline;
  font-family: Consolas;
  font-size: 14px; }

.ruzzle-cell-points-sd {
  left: 60px; }

.ruzzle-cell-points-dd {
  left: 61px; }

.ruzzle-cell-special-tag {
  position: absolute;
  top: -40px;
  left: 0px;
  display: inline;
  background-color: @ruzzleColor;
  color: white;
  padding: 2px;
  font-family: Consolas;
  font-size: 12px; }

.ruzzle-result {
  min-height: 150px; }

.ruzzle-summary-title {
  text-transform: uppercase;
  font-weight: 600;
  float: left;
  width: 150px;
//  padding-bottom: 20px; }
}

.ruzzle-summary-data {
  text-transform: uppercase;
  font-weight: 600;
  float: left;
  position: relative;
  left: 45px; }

