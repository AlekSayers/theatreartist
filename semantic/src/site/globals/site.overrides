#line00to01, #line01to02, #line02to03,
#line10to11, #line11to12, #line12to13,
#line20to21, #line21to22, #line22to23,
#line30to31, #line31to32, #line32to33,
#line00to10, #line10to20, #line20to30,
#line01to11, #line11to21, #line21to31,
#line02to12, #line12to22, #line22to32,
#line03to13, #line13to23, #line23to33,
#line00to11, #line11to22, #line22to33,
#line10to21, #line21to32,
#line20to31,
#line01to12, #line12to23,
#line02to13,
#line30to21, #line21to12, #line12to03,
#line20to11, #line11to02,
