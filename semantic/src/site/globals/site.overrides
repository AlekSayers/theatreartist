/*******************************
         Site Overrides
*******************************/

@ruzzleColor: @primaryColor;
 
input.ruzzle-cell,
input.ruzzle-cell-special,
input.ruzzle-cell-small,
input.ruzzle-cell-small-hit {
  border: solid 1px #BFBDBD;
  color: #ffffff;
  /*box-shadow: 2px 2px 5px #ffffff;*/
  border: 0;
  text-transform: capitalize;
  text-align: center;
  font-family: Tahoma;
  font-weight: 800;
  //margin-right: -1px;
  margin-right: 2px;
  margin-bottom: 1px;
  padding: 0px; }

input.ruzzle-cell-small,
input.ruzzle-cell-small-hit {
  margin-bottom: 1px; }

input.ruzzle-cell,
input.ruzzle-cell-special {
  height: 70px;
  width: 70px; }

input.ruzzle-cell-small,
input.ruzzle-cell-small-hit {
  height: 20px;
  width: 20px;
  margin-right: 1px; }

input.ruzzle-cell {
  background-color: #f0f0f0;
  font-size: 54px;
  maxlength: 1;
  color: #272727; }

input.ruzzle-cell-special {
  background-color: @ruzzleColor;
  font-size: 36px; }

input.ruzzle-cell-small {
  background-color: #272727; }

input.ruzzle-cell-small-hit {
  background-color: #f0f0f0; }

#line00to01, #line01to02, #line02to03,
#line10to11, #line11to12, #line12to13,
#line20to21, #line21to22, #line22to23,
#line30to31, #line31to32, #line32to33 {
  width: 71px;
  height: 4px; }

#line00to10, #line10to20, #line20to30,
#line01to11, #line11to21, #line21to31,
#line02to12, #line12to22, #line22to32,
#line03to13, #line13to23, #line23to33 {
  width: 4px;
  height: 71px; }

#line00to11, #line11to22, #line22to33,
#line10to21, #line21to32,
#line20to31,
#line01to12, #line12to23,
#line02to13 {
  transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  height: 4px;
  width: 100px; }

#line30to21, #line21to12, #line12to03,
#line20to11, #line11to02,
#line10to01,
#line31to22, #line22to13,
#line32to23 {
  transform: rotate(-45deg);
  -ms-transform: rotate(-45deg);
  -moz-transform: rotate(-45deg);
  -webkit-transform: rotate(-45deg);
  -o-transform: rotate(-45deg);
  height: 4px;
  width: 100px; }

